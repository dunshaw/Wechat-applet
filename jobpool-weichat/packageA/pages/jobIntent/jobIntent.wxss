/* pages/jobIntent/jobIntent.wxss */
.selectedShell{
  width: 100%;
  height: 211rpx;
  overflow: hidden;
  margin: 0;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  border-bottom: 1rpx solid #f5f5f5;
  background-color: #fff;
  z-index: 10;
}
.selectedTitle{
  width: 590rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  float: left;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
}
.selectedNub{
  width: 100rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  float: right;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  text-align: right;
}
.selectedNub>text{
  color: #2EA7E0;
}
.selectedBtnShell{
  width: 100%;
  overflow: hidden;
  padding: 0;
  margin: 30rpx 0 0 0;
  float: left;
}
.noBtn{
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}
.haveBtn{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
}
.btn{
  width: 200rpx;
  height: 50rpx;
  margin: 0 30rpx 0 0;
  padding: 12rpx 10rpx;
  border: 1rpx solid #2EA7E0;
  box-sizing: border-box;
  border-radius: 6rpx;
  float: left;
}
.btnCon{
  width: 146rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #2EA7E0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}
.btn>.fa-close{
  width: 30rpx;
  font-size: 24rpx;
  color: #2EA7E0;
  float: left;
  text-align: right;
  font-weight: normal;
}
.middleConShell{
  width: 100%;
  height: calc(100% - 300rpx);
  overflow: hidden;
  padding: 0;
  box-sizing: border-box;
  position: fixed;
  top: 211rpx;
  z-index: 5;
}
.middleLeft{
  width: 325rpx;
  height: 100%;
  margin: 0;
  padding: 0;
  float: left;
  background-color: #F5F5F5;
  box-sizing: border-box;
}
.leftCon{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 40rpx 30rpx;
  font-size: 28rpx;
  color: #666;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
.active{
  background-color: #fff;
  color: #2EA7E0;
}
.middleRight{
  width: 425rpx;
  height: 100%;
  margin: 0;
  padding: 0;
  float: left;
  box-sizing: border-box;
}
.rightModel{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 30rpx;
  box-sizing: border-box;
}
.modelTitle{
  font-size: 24rpx;
  color: #999999;
  line-height: 24rpx;
  border-left: 2px solid #2EA7E0;
  padding-left: 10rpx;
  margin-bottom: 30rpx;
}
.modelOption{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 30rpx 10rpx;
  font-size: 28rpx;
  color: #666;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
.keepSelected{
  width: 100%;
  height: 88rpx;
  margin: 0;
  padding: 0;
  position: fixed;
  bottom: 0;
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 88rpx;
  text-align: center;
  z-index: 100;
  background-color: #2EA7E0;
}
 





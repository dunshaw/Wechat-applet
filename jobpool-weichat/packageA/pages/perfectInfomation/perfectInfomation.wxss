/* pages/perfectInfomation/perfectInfomation.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.Shell{
  width: 100%;
  margin: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.genderModel{
  width: 100%;
  height: 120rpx;
  margin:0;
  padding: 40rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-content: center;
}
.ganderTitle{
  width: 30%;
  height: 32rpx;
  padding: 0;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  text-align: left;
  float: left;

}
.xinghaored{
  width: 20rpx;
  height: 20rpx;
}
.mailboxView{
  width: 70%;
  font-size: 32rpx;
  color: #666;
  line-height: 32rpx;
}
.inputName{
  width: 100%;
  min-height: 32rpx;
  font-size: 30rpx;
  color: #666;
  line-height: 32rpx;
  margin: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  text-align: right;
}
.submitForm{
  width: 690rpx;
  height: 80rpx;
  margin: 0 auto;
  padding: 0;
  font-size: 32rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  background-color: #2EA7E0;
  border-radius: 10rpx;
  position: fixed;
  bottom: 100rpx;
  z-index: 100;

}
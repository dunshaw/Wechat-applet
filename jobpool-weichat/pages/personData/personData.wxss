/* pages/personData/personData.wxss */
page{
  background-color: #f5f5f5;
}
.Shell{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.secondShell{
  margin: 20rpx 0;
}
.headPicModel{
  width: 100%;
  height: 140rpx;
  overflow: hidden;
  margin: 0;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
}
.headTitle{
  width: 180rpx;
  height: 100rpx;
  margin: 0 20rpx 0 0;
  padding: 0;
  font-size: 32rpx;
  line-height: 100rpx;
  color: #333;
  text-align: left;
  float: left;
}
.headImgView{
  width: 490rpx;
  height: 100rpx;
  margin: 0;
  padding: 0;
  text-align: right;
  float: right;
}
.headImg{
  width: 100rpx;
  height: 100rpx;
  margin: 0;
  padding: 0;
  vertical-align: middle;
  border-radius: 50%;
}
.nameModel,.genderModel,.mailboxModel{
  width: 100%;
  height: 112rpx;
  overflow: hidden;
  margin: 0;
  padding: 40rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
}
.nameModel,.mailboxModel{
  height: 134rpx;
}
.mailboxModel,.genderModel:last-child{
  border: none;
}
.nameTitle{
  width: 200rpx;
  height: 54rpx;
  margin: 0 20rpx 0 0;
  padding: 0;
  font-size: 32rpx;
  line-height: 54rpx;
  color: #333;
  text-align: left;
  float: left;
}
.genderTitle,.mailboxTitle{
  width: 200rpx;
  height: 32rpx;
  margin: 0 20rpx 0 0;
  padding: 0;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  text-align: left;
  float: left;
}
.mailboxTitle{
  height: 54rpx;
  line-height: 54rpx;
}
.nameView,.genderView,.mailboxView{
  width: 470rpx;
  height: 32rpx;
  margin: 0;
  padding: 0;
  text-align: right;
  float: right;
}
.nameView,.mailboxView{
  height: 54rpx;
}
.inputName{
  width: 100%;
  min-height: 32rpx;
  font-size: 32rpx;
  color: #666;
  line-height: 32rpx;
  margin: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.placeholder{
  font-size: 32rpx;
  line-height: 32rpx;
  color: #999;
}
.genderView,.mailboxView{
  font-size: 32rpx;
  color: #666;
  line-height: 32rpx;
}
.notChoice{
  color: #999;
}
.genderView>.fa{
  margin-left: 15rpx;
  color: #999;
}

/*选择地址*/
.address-mask{
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 98;
  position: fixed;
}
.picker-view{
  width: 100%;
  height: 300px;
  position: absolute;
  bottom: 0;
  z-index: 99;
  background: #fff;
}
.operation{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  font-size: 30rpx;
  position: absolute;
  z-index: 99;
  box-sizing: border-box;
  background: #fff;
}
.operation .cancel-btn{
  color: #999;
}
.operation .comfirm-btn{
  color: goldenrod;
  float: right;
}
.item-column{
  line-height: 50px;
  font-size: 26rpx;
  text-align: center;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.submitForm{
  width: 690rpx;
  height: 80rpx;
  margin: 30rpx;
  padding: 0;
  font-size: 32rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  background-color: #2EA7E0;
  border-radius: 10rpx;
  position: fixed;
  bottom: 20rpx;
  z-index: 100;
}
.xinghaored{
  width: 20rpx;
  height: 20rpx;
}
.fjAuthModel{
  width: 100%;
  height: 450rpx;
}
.renzhengbeijing{
  width: 100%;
  height: 300rpx;
  margin: 20rpx 0;
}
.yirenzheng{
  width: 124rpx;
  height: 44rpx;
  position: absolute;
  right:8% ;
  top: 50rpx;
  z-index: 5;
}
.renzhengbaozhangview{
  font-size:26rpx ;
  color: #fff;
  position: absolute;
  right: 8%;
  top: 275rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.renzhengbaozhang{
  width: 22rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.renzhengxinxi{
  position: absolute;
  height: 200rpx;
  top: 50rpx;
  left: 10%;
  color: #fff;
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}
.leftrenzheng{width: 80%;}
.fjusersex{float: right;}
.fjuseryear{margin-bottom: 10rpx;}
.fjusername{
  font-size: 44rpx;
  font-weight: bold;
}
.shimingtishi{
  font-size: 26rpx;
  color: #999;
  display: block;
}
.fjuseryear{
  display: block;
}








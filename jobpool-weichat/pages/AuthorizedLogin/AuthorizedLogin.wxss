.q-loginlogo{
  width: 450rpx;
  height: 106rpx;
  margin: 240rpx auto 0;
}
.q-loginlogo image{
  width: 100%;
  height: 100%;
}
.q-weixinlogin{
  height: 80rpx;
  margin: 100rpx 30rpx 0;
  display: flex;
  justify-content: center;

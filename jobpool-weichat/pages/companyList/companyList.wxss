/* pages/companyList/companyList.wxss */
.header{
  height: 128rpx;
  width: 100%;
  background:#EDF6FA;
}
.search{
  width: 100%;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.searchbar-icon{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 50rpx;
  left: 5%;
}
.searchbar-input{
  height: 68rpx;
  background: #fff;
  font-size: 26rpx;
  width: 690rpx;
  margin: auto;
  border-radius: 50rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.clearsearch{
  color: #2CA7E0;
  font-size: 30rpx;
  float: right;
  margin-right: 20rpx;
  line-height: 128rpx;
}

.filterLable{
  height: 88rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #666;
  text-align: center;
  border-bottom: 1px solid #F0F0F0;
}
.filter{
  flex: 1;
  position: relative;
}
.filter>.fa{
  width: 20rpx;
  height: 20rpx;
  color: #999;
  position: absolute;
  margin-left: 10rpx;
}
.active{
  color: #2CA7E0;
}
.active>.fa{
  color: #2CA7E0;
}
.automatic,.autoscreen{
  width: 100%;
  height: max-content;
  margin: 0;
  background:#fff;
}
.autoMode{
  width: 100%;
  height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 90rpx;
  color: #666;
  border-bottom: 1px solid #F0F0F0;
}
.addressName:last-child,.timeSlot:last-child,.autoMode:last-child{
  border-bottom: none;
}
.scrollbackbg{
  background:rgba(0, 0, 0, .5);
}
.autoscreen{
  padding: 10rpx;
}
.modelTitle{
  width: 100%;
  font-size: 32rpx;
  color: #333;
  padding: 20rpx 10rpx 6rpx 15rpx;
}
.modelCon{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
}
.modelConName{
  width: max-content;
  height: 60rpx;
  margin: 24rpx 10rpx 0 10rpx;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 60rpx;
  text-align: center;
  background-color: #F8F8F8;
  border-radius: 30rpx;
  float: left;
  box-sizing: border-box;
}
.modelConName{
  width: max-content;
  height: 60rpx;
  margin: 14rpx 10rpx 10rpx 10rpx;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 60rpx;
  text-align: center;
  background-color: #F8F8F8;
  border-radius: 30rpx;
  float: left;
  box-sizing: border-box;
}
.checked{
  color: #fff;
  background-color: #2CA7E0;
}
.sureBtn{
  width: 100%;
  height: 88rpx;
  margin: 0;
  padding: 0;
  border-top: 1px solid #f1f1f1;
  box-sizing: border-box;
  background:#f1f1f1;
  color: #999;
  /* position: absolute;
  bottom: 125rpx; */
}
.cancel,.sure{
  width: 50%;
  height: 100%;
  margin: 0;
  padding: 0;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  float: left;
}
.sure{
  color: #fff;
  background-color: #2EA7E0;
}
.selectbox{
  position: absolute;
  z-index: 10;
  /* display: none; */
}
.companybox{
  background:#f5f5f5;
}
.companyItem{
  width: 100%;
  height: 198rpx;
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  background:#fff;
}
.companyImg{
  width: 80rpx;
  height: 80rpx;
  margin:10rpx;
}
.company-right{
  width: 160rpx;
  font-size: 24rpx;
  color: #ff961e;
  margin:10rpx;
  text-align: right;
}
.company-content{
  margin:10rpx;
  flex:1;
  width: 0;
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  justify-content: space-between;
}
.company-content h3{
  font-weight: bold;
  font-size: 36rpx;
  color: #333;
  max-width: 65%;
  margin-right: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.company-content-first{
  display: flex;
  align-items: center;
}
.company-content-secend{
  width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  color: #999;
}
.company-content-third{
  color: #666;
  width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* overflow: hidden; */
}
.company-content-third span{
  color: #999;
  margin:0 10rpx;
}
.zizhirenzheng{
  font-size: 12px;
  padding: 2rpx 10rpx;
  display: inline-block;
  background:rgba(244,251,253,1);
  border-radius:4px;
  border:1px solid rgba(46,167,224,1);
  color: #2CA7E0;
}
.islogin{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  z-index: 1000;
  background:#fff;
}
.islogin image{
  width: 590rpx;
  height: 430rpx;
}
.islogin text{
  margin-top: 60rpx;
  padding: 0 30rpx;
  font-size:16px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:30px;
  border: 2rpx solid rgba(102,102,102,1);
  border-radius: 30rpx;
}
.automatic .active{
  color: #2CA7E0;
}
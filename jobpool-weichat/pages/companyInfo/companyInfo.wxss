  border-bottom: 6rpx solid #2EA7E0;
  color: #2EA7E0;
}
.isShowThis{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
}
.showModel{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 50rpx 30rpx 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.comIntro{
  width: 100%;
  margin: 0;
  padding: 0;
  font-size: 28rpx;
  color: #333;
  line-height: 48rpx;
}
.sdown{
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:4;
}
.seedown{
  margin: 0 auto;
  width: 24rpx;
  height: 24rpx;
  transform: rotate(-45deg);
  border-left: 4rpx solid #2EA7E0;
  border-bottom: 4rpx solid #2EA7E0;
}
.nosdown{
  transform: rotate(135deg);
}
.q-line{
  margin: 40rpx 0;
  height: 1px;
  background: #ddd
}
.seeMore{
  display: block;
  text-align: center;
  font-size: 56rpx;
  color: #2EA7E0;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #ddd;
}
.addressLocation{
  font-size: 28rpx;
  color: #333;
  padding: 30rpx 0
}
.addressLocation>.fa{
  color: #FF961E;
  margin-right: 15rpx;
}
.typeAndTime{
  width: 100%;

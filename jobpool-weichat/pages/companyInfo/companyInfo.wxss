  margin: 20rpx 0 0 0;
  padding: 60rpx 30rpx 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.typeAndTime>view{
  font-size: 28rpx;
  color: #333;
  margin-bottom: 30rpx;
  padding: 0;
  box-sizing: border-box;
}
.typeAndTime>view>text{
  color: #999;
}
.typeAndTime>view.eyesCheck{
  color: #999;
  text-align: right;
  margin: 80rpx 0 0 0;
  padding: 0 0 30rpx 0;
  border-bottom: 1rpx solid #ddd;
}
.eyes{
  width: 100rpx;
  height: 28rpx;
  margin: 0;
  padding: 0;
  vertical-align: middle;
}
.eyesCheckMore{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 30rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #2EA7E0;
  box-sizing: border-box;
}
.eyesCheckMore>.fa{
  float: right;
}



/* pages/searchResult/searchResult.wxss */
.filterLable{
  width: 100%;
  overflow: hidden;
  margin: 20rpx 0 0 0;
  padding: 0;
  background-color: #fff;
  border-bottom: 1px solid #DDDDDD;
}
.topnav{
    position: fixed;
    top: 0;
    z-index:200;
    margin: 0;
}
.filter{
  width: 25%;
  overflow: hidden;
  margin: 0;
  padding: 32rpx 50rpx 32rpx 0;
  text-align: right;
  float: left;
  position: relative;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #666;
}
.filter:first-child{width: 30%;}
.filter:nth-child(3){width: 20%;}
.filter:nth-child(4){width: 20%;}
.filter>.fa{
  position: absolute;
  right: 25rpx;
  color: #999;
}
.scene-top{
  width: 100%;
  height: 122rpx;
  display: flex;
  background:rgba(237,246,250,1);
  position: fixed;
  top: 0;
  left: 0;
  z-index:201;
}
.scene-all,.scene-date{
  flex-grow: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:40rpx;
}
.scene-date{
  flex-direction: column;
}
.scene-date-d{
  font-size:24rpx;
}
.scene-top-bg{
  color:rgba(255,255,255,1);
  background:rgba(46,167,224,1);
}
.content{
  width: 100%;
  overflow: hidden;
  margin: 0 0 20rpx 0;
  padding: 30rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.information{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
  display: flex;
}
.leftCon{
  width: 480rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  float: left;
}
.rightCon{
  width: 210rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  float: left;
}
.imgAndTitle{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
  position: relative;
}
.tuijianclass{
  width: 55rpx;
  height: 30rpx;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
}
.compan-box{
  display: flex;
}
.companypg,.companyhot{
  width: 32rpx;
  height: 32rpx;
  margin: 2rpx;
}
.companyIcon{
  width: 88rpx;
  height: 88rpx;
  float: left;
}
.Title{
  max-width: 372rpx;
  height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 20rpx;
  padding: 0;
  font-size: 32rpx;
  color: #333;
  line-height: 36rpx;
  font-weight: bold;
  float: left;
}
.companyName{
  width: 372rpx;
  height: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 15rpx 0 0 20rpx;
  padding: 0;
  font-size: 28rpx;
  color: #333;
  line-height: 32rpx;
  float: left;
}
.require{
  width: 100%;
  overflow: hidden;
  margin: 30rpx 0 0 0;
  padding: 0;
  font-size: 24rpx;
  color: #666;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.salary{
  height: 32rpx;
  font-size: 26rpx;
  color: #FF961E;
  text-align: right;
  font-weight: bold;
  line-height: 32rpx;
  margin-bottom: 15rpx;
}
.typeShell{
  width: 100%;
  overflow: hidden;
  margin: 0 0 15rpx 0;
  padding: 0;
}
.types{
  width: max-content;
  font-size: 20rpx;
  text-align: right;
  padding: 11rpx;
  float: right;
}
.type1{
  border: 1px solid #FFAC4C;
  border-radius: 6rpx;
  color: #FFAC4C;
}
.type2{
  border: 1px solid #70CC70;
  border-radius: 6rpx;
  color: #70CC70;
}
.m_reward,.w_reward,.r_reward{
  /* display: inline-block; */
  width: 122rpx;
  font-size: 20rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
  text-align: center;
  margin-right: 20rpx;
}
.m_reward{
  border: 1px solid #45B3E6;
  border-radius: 6rpx;
  color: #45B3E6;
}
.w_reward{
  border: 1px solid #FF6E6D;
  border-radius: 6rpx;
  color: #FF6E6D;
}
.r_reward{
  border: 1px solid #FF961E;
  border-radius: 6rpx;
  color: #FF961E;
}
.welfareShell{
  width: 480rpx;
  overflow: hidden;
  margin: 10rpx 0 0 0;
  /* padding: 0 0 30rpx 0; */
  border-bottom: 1px solid #DDDDDD;
}
/* .q-cheap{
  margin-top: -40rpx;
} */
.welfare{
  display: inline-block;
  max-width: 144rpx;
  /*height: 44rpx;*/
  margin: 0 10rpx 10rpx 0;
  padding: 10rpx;
  font-size: 24rpx;
  line-height: 28rpx;
  color: #999;
  background-color: #f8f8f8;
  border-radius: 6rpx;
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.timeAndNumber{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 30rpx 0 0 0;
}
.time{
  display: inline-block;
  width: 500rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  font-size: 24rpx;
  color: #666;
  text-align: left;
}
.number{
  display: inline-block;
  width: 190rpx;
  overflow: hidden;
  margin: 0;
  padding: 0;
  font-size: 24rpx;
  color: #666;
  text-align: right;
}
.time>text,.number>text{
  color: #2EA7E0;
}

.setUpShell{
  width: 100%;
  overflow: hidden;
  margin: 0 0 20rpx 0;
  padding: 0;
  position: relative;
}
.Recommend{
  width: 100%;
  height: 120rpx;
}
.setUp{
  width: max-content;
  height: 30rpx;
  font-size: 28rpx;
  line-height: 30rpx;
  color: #fff;
  position: absolute;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}
.setUp>text{
  color: #2EA7E0;
  font-weight: bold;
}
.vitaShell{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: fixed;
  top: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 9999 !important;
}
.vitaCon{
  width: 438rpx;
  height: max-content;
  margin: auto;
  padding: 20rpx 20rpx 80rpx 20rpx;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
}
.conView{
  width: 100%;
  height: 410rpx;
  margin: 0;
  padding: 0;
  border-radius: 18rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.conViewImg{
  width: 100%;
  height: 160rpx;
  border-radius: 18rpx 18rpx 0 0;
}
.conViewP1{
  height: 28rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  line-height: 28rpx;
  margin: 39rpx auto 0 auto;
}
.conViewP2{
  height: 24rpx;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  line-height: 24rpx;
  margin: 20rpx auto 0 auto;
}
.toPerfect{
  width: 235rpx;
  height: 64rpx;
  margin: 40rpx auto 0 auto;
  padding: 0;
  font-size: 28rpx;
  color: #f8f8f8;
  line-height: 64rpx;
  text-align: center;
  background: linear-gradient(to right, #2EA7E0 , #8DDBFF); 
  border-radius: 32rpx;
  box-shadow:0px 3px 6px 0px rgba(0, 0, 0, 0.2);
}
.toPerfectIcon{
  width: 231rpx;
  height: 209rpx;
  margin: auto;
  padding: 0;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}
.closeThis{
  width: 48rpx;
  height: 48rpx;
  margin: auto;
  position: absolute;
  padding: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.screenCondition{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: fixed;
  top: 0;
  background-color: #fff;
  z-index: 1000;
}
.allAddress,.choiceTime,.automatic{
  width: 100%;
  height: max-content;
  margin: 0;
  padding: 0 0 125rpx 0;
}
.screen{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0 0 100rpx 0;
  position: relative;
}
.addressName,.timeSlot,.autoMode{
  width: 100%;
  height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 90rpx;
  color: #666;
  border-bottom: 1px solid #DDDDDD;
}
.addressName:last-child,.timeSlot:last-child,.autoMode:last-child{
  border-bottom: none;
}
.active{
  color: #2EA7E0;
}
.active>.fa{
  color: #2EA7E0;
}
.model{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
}
.modelTitle{
  width: 100%;
  font-size: 32rpx;
  color: #333;
  margin: 20rpx 10rpx 6rpx 10rpx;
}
.modelCon{
  width: 100%;
  overflow: hidden;
  margin: 0;
  padding: 0;
}
.modelConName{
  width: max-content;
  height: 60rpx;
  margin: 24rpx 10rpx 0 10rpx;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 60rpx;
  text-align: center;
  background-color: #F8F8F8;
  border-radius: 30rpx;
  float: left;
  box-sizing: border-box;
}
.checked{
  color: #fff;
  background-color: #FF961E;
}
.sureBtn{
  width: 100%;
  height: 88rpx;
  margin: 0;
  padding: 0;
  border-top: 1px solid #DDDDDD;
  box-sizing: border-box;
  margin-top: 10px;
  background:#fff;
}
.cancel,.sure{
  width: 50%;
  height: 100%;
  margin: 0;
  padding: 0;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: #333;
  float: left;
}
.sure{
  color: #fff;
  background-color: #2EA7E0;
}
.q-partitionline{
    width: 100%;
    height: 20rpx;
    margin: 0;
    padding: 0;
    background-color: #f5f5f5;
}
.q-nothing{
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.q-nothing image{
  width: 590rpx;
  height: 430rpx;
}
.q-nothing view{
  margin-top: 20rpx;
  font-size:16px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:30px;
}
.wm_reward{
  display: flex;
  flex-direction: row;
  width: 100%;
  /* justify-content: start; */
  justify-items: flex-start;
  margin: 20rpx 0;
}
.salaryshow{
  width: 100%;
  text-align: center;
  display: block;
  font-size: 26rpx;
  color: #333;
  margin: 10rpx 0;
}